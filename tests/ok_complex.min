# Programa mais complexo: encontrar números primos
function ehPrimo(int n): bool {
    if (n <= 1) {
        return false;
    }
    
    if (n <= 3) {
        return true;
    }
    
    int i = 2;
    while (i * i <= n) {
        if (n % i == 0) {
            return false;
        }
        i = i + 1;
    }
    
    return true;
}

print("Números primos até 20:");

for (int num = 2; num <= 20; num = num + 1) {
    if (ehPrimo(num)) {
        print(num);
    }
}